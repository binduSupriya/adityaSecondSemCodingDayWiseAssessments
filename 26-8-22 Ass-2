#include<stdio.h>
void main()
{
	char str[100];
	gets(str);
	int i=0,j,flag=1;
	for(j=0;str[j]!='\0';j++)
	{
		
	}
	j--;
	while(i<j)
	{
		if(str[i]!=str[j])
		{
			flag=0;
			break;
		}
		i++;
		j--;
	}
	if(flag)
		printf("Palindrome");
	else
		printf("Not a Palindrome");	
}

/*
OUTPUT:

mom
Palindrome

mother
Not a Palindrome
*/
