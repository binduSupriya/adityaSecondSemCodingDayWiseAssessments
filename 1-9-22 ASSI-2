#include<stdio.h>
void main()
{
	int n;
	printf("Enter the number: ");
	scanf("%d",&n);
	int fact=factorial(n);
	printf("Factorial of %d is %d",n,fact);
}
int factorial(int n)
{
	int i,fact=1;
	for(i=n;i>=1;i--)
	{
		fact=fact*i;
	}
	return fact;
}


/*

OUTPUT:

Enter the number: 8
Factorial of 8 is 40320
*/
